{
  "name": "solmate",
  "version": "2.0.0",
  "description": "Solmate - Your AI-powered Solana companion with 3D VRM avatar, chat, TTS, and live blockchain data",
  "private": true,
  "type": "module",
  "author": "DirkDigglerTown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DirkDigglerTown/Solmate.git"
  },
  "keywords": [
    "solana",
    "ai",
    "chat",
    "vrm",
    "3d",
    "avatar",
    "tts",
    "blockchain",
    "crypto",
    "web3"
  ],
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vercel dev --listen 3000",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:api": "node scripts/test-api.js",
    "lint": "eslint . --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .vercel node_modules",
    "clean:cache": "rm -rf .vite node_modules/.vite",
    "analyze": "vite-bundle-visualizer",
    "security:check": "npm audit --audit-level=moderate",
    "env:pull": "vercel env pull .env.local",
    "env:add": "vercel env add",
    "precommit": "npm run lint && npm run format:check && npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "three": "^0.169.0",
    "@pixiv/three-vrm": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/three": "^0.169.0",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "terser": "^5.27.0",
    "typescript": "^5.3.0",
    "vercel": "^33.0.0",
    "vite": "^5.0.0",
    "vite-bundle-visualizer": "^1.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  }
}
