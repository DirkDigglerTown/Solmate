{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/web/api/$1.js"
    },
    {
      "source": "/manifest.webmanifest",
      "destination": "/web/manifest.webmanifest"
    },
    {
      "source": "/(.*)",
      "destination": "/web/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "/assets/avatar/solmate.vrm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ],
  "functions": {
    "web/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
